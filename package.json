{
  "name": "create-react-app-with-eel",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "chart.js": "^4.4.1",
    "leaflet": "^1.9.4",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "^18.2.0",
    "react-leaflet": "^4.2.1",
    "react-router-dom": "^6.21.3",
    "react-scripts": "5.0.1"
  },
  "scripts": {
    "start:js": "react-scripts start",
    "build:js": "react-scripts build",
    "start:eel": "python3 index.py --develop",
    "build:eel": "python3 -m eel index.py build --onefile --noconsole --name react-eel-app",
    "clean:build": "rm -rf build dist",
    "start": "npm-run-all -p start:*",
    "build": "npm-run-all clean:build build:*",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
